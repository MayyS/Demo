<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="home" extends="struts-default" namespace="/">
        <action name="home_*" class="edu.action.HomeAction" method="{1}">

            <result name="{1}">/jsp/admin/home/{1}.jsp</result>
            <result name="myIndex">/jsp/font/{1}.jsp</result>
            <result name="forget">/jsp/font/{1}.jsp</result>
            <result name="changePwd">/jsp/admin/user/{1}.jsp</result>
            <result name="success">/jsp/font/{1}.jsp</result>
            <result name="input">/jsp/font/userLog.jsp</result>
        </action>
    </package>
</struts>